{
  "expo": {
    "name": "cm-mobile-stack",
    "slug": "cm-mobile-stack",
    "scheme": "dogstack",
    "deepLinks": [
      "dogstack://"
    ],
    "version": "0.0.10",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo-color.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a1a"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "statusBar": {
        "translucent": false,
        "backgroundColor": "#ffffff"
      },
      "package": "com.mcnamaracharles.cmmobilestack"
    },
    "androidNavigationBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#ffffff",
      "visible": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "environment": "development",
      "eas": {
        "projectId": "05380a06-224a-408e-8289-5dd1653c88da"
      }
    }
  }
}
